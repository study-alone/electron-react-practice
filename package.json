    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
